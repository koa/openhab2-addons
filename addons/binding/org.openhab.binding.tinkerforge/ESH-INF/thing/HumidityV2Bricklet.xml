			</parameter>
	
			<parameter name="movingAverageLengthTemperature" type="Integer" required="false">
				<label>movingAverageLengthTemperature</label>
				<description>movingAverageLengthTemperature.</description>
			</parameter>
	
		</config-description>

	</thing-type>


